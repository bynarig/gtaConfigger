{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx -r tsconfig-paths/register  src/app.ts --watch",
    "test": "jest",
    "lint": "eslint src",
    "prod": "esbuild src/app.ts --bundle --platform=node --outdir=dist --minify --sourcemap && node ./dist/app.js",
    "pretty": "prettier './**/**.{ts,tsx,js,jsx,mjs,cjs,css,scss,sass,md,*.json,.prettierrc,.lintstagedrc,.babelrc,.eslintrc,*.webmanifest}'",
    "format": "pnpm pretty --write"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1",
    "passport-google-oauth20": "^2.0.0",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "serve": "^14.2.4",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslint-config-prettier": "^1.18.0",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.25.0"
  },
  "imports": {
    "#utils/*": "./src/utils/*",
    "#controllers/*": "./src/controllers/*",
    "#data/*": "./src/data/*",
    "#dtos/*": "./src/dtos/*",
    "#models/*": "./src/models/*",
    "#middlewares/*": "./src/middlewares/*",
    "#tests/*": "./tests/*"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "bcrypt"
    ]
  }
}
